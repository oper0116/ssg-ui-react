{
  "name": "ssg-ui-react",
  "version": "0.1.0",
  "description": "SSG React UI components built with React and Chakra UI",
  "repository": "https://bit.ssgadm.com/scm/frontmobile/ssg-ui-react.git",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12.x",
    "yarn": ">=1.22.x"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run type-check",
      "commit-msg": "yarn commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint"
    ]
  },
  "scripts": {
    "postinstall": "husky install",
    "clean": "rimraf dist",
    "build": "yarn clean && rollup -c",
    "build:watch": "yarn clean && rollup -cw",
    "prepare": "yarn build",
    "test": "jest --env=jsdom --passWithNoTests",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.1",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "bimap": "^0.0.15",
    "dayjs": "^1.10.7",
    "framer-motion": "^5.3.0",
    "swiper": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@chakra-ui/babel-plugin": "^1.0.0",
    "@chakra-ui/theme-tools": "^1.3.1",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-actions": "next",
    "@storybook/addon-essentials": "next",
    "@storybook/addon-links": "next",
    "@storybook/addon-storysource": "next",
    "@storybook/addons": "next",
    "@storybook/builder-webpack5": "next",
    "@storybook/core": "next",
    "@storybook/manager-webpack5": "next",
    "@storybook/react": "next",
    "@storybook/theming": "next",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "next": "^12.0.4",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.31.0",
    "ts-jest": "^27.0.7",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.1.2",
    "webpack": "^5.64.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.1.0",
  "peerDependencies": {
    "next": "12.x",
    "react": "17.x",
    "react-dom": "17.x"
  }
}
